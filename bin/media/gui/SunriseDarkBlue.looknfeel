    <!--
    ***************************************************
    Sunrise/TabControl
    ***************************************************
    -->
    <WidgetLook name="Sunrise/TabControl">

        <Property name="TabButtonType" value="Sunrise/TabButton" />
        <Property name="TabTextPadding" value="{0,6}" />
		<PropertyDefinition name="ScrollbuttonWidth" initialValue="20" layoutOnWrite="true" redrawOnWrite="true" />

        <Child type="Sunrise/TabContentPane" nameSuffix="__auto_TabPane__">
            <Area>
                <Dim type="LeftEdge"><AbsoluteDim value="0" /></Dim>
                <Dim type="RightEdge"><UnifiedDim scale="1" type="RightEdge" /></Dim>
                <Dim type="TopEdge">
                    <PropertyDim widget="__auto_TabPane__" name="EnableBottom">
      		                <DimOperator op="Multiply">
              		            <PropertyDim name="TabHeight" type="Height" />
                      		</DimOperator>
					</PropertyDim>
                </Dim>
                <Dim type="Height">
                    <UnifiedDim scale="1" type="Height">
                        <DimOperator op="Subtract">
                            <PropertyDim name="TabHeight" type="Height" />
                        </DimOperator>
                    </UnifiedDim>
                </Dim>
            </Area>
        </Child>

        <Child  type="Sunrise/TabButtonPane" nameSuffix="__auto_TabPane__Buttons">
            <Area>
                <Dim type="LeftEdge">
					<PropertyDim name="ScrollbuttonWidth" />
				</Dim>
                <Dim type="RightEdge">
					<UnifiedDim scale="1" type="RightEdge">
						<DimOperator op="Subtract">
							<PropertyDim name="ScrollbuttonWidth" />
						</DimOperator>
					</UnifiedDim>
				</Dim>
                <Dim type="TopEdge">
                    <PropertyDim widget="__auto_TabPane__Buttons" name="EnableTop">
                        <DimOperator op="Multiply">
                            <UnifiedDim scale="1" type="Height">
                                <DimOperator op="Subtract">
                                    <PropertyDim name="TabHeight" type="Height" />
                                </DimOperator>
                            </UnifiedDim>
                        </DimOperator>
                    </PropertyDim>
                </Dim>
                <Dim type="Height">
					<PropertyDim name="TabHeight" type="Height" />
				</Dim>
            </Area>
        </Child>

        <Child type="Sunrise/IconButton" nameSuffix="__auto_TabPane__ScrollLeft">
            <Area>
                <Dim type="LeftEdge"><AbsoluteDim value="0" /></Dim>
                <Dim type="TopEdge">
                    <WidgetDim widget="__auto_TabPane__Buttons" dimension="TopEdge" />
                </Dim>
                <Dim type="Width">
                    <PropertyDim name="ScrollbuttonWidth" />
                </Dim>
                <Dim type="Height">
                    <WidgetDim widget="__auto_TabPane__Buttons" dimension="Height" />
                </Dim>
            </Area>
            <Property name="AlwaysOnTop" value="True" />
            <Property name="IconImage" value="set:Sunrise image:LargeArrowLeft" />
        </Child>

        <Child type="Sunrise/IconButton" nameSuffix="__auto_TabPane__ScrollRight">
            <Area>
                <Dim type="LeftEdge">
                    <WidgetDim widget="__auto_TabPane__" dimension="RightEdge">
                        <DimOperator op="Subtract">
                            <PropertyDim name="ScrollbuttonWidth" />
                        </DimOperator>
                    </WidgetDim>
                </Dim>
                <Dim type="TopEdge">
                    <WidgetDim widget="__auto_TabPane__Buttons" dimension="TopEdge" />
                </Dim>
                <Dim type="Width">
                    <PropertyDim name="ScrollbuttonWidth" />
                </Dim>
                <Dim type="Height">
                    <WidgetDim widget="__auto_TabPane__Buttons" dimension="Height" />
                </Dim>
            </Area>
            <Property name="AlwaysOnTop" value="True" />
            <Property name="IconImage" value="set:Sunrise image:LargeArrowRight" />
        </Child>

        <StateImagery name="Enabled" />

        <StateImagery name="Disabled" />

    </WidgetLook>

