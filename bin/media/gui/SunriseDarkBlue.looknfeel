<?xml version="1" encoding="UTF-8"?>
<Falagard>

    <!--
    ***************************************************
    Sunrise/StaticShared
    ***************************************************
    -->
    <WidgetLook name="Sunrise/Shared">

        <ImagerySection name="frame">
            <FrameComponent>
                <Area>
                    <Dim type="LeftEdge">
                        <AbsoluteDim value="0" />
                    </Dim>
                    <Dim type="TopEdge">
                        <AbsoluteDim value="0" />
                    </Dim>
                    <Dim type="RightEdge">
                        <UnifiedDim scale="1" type="RightEdge" />
                    </Dim>
                    <Dim type="BottomEdge">
                        <UnifiedDim scale="1" type="BottomEdge" />
                    </Dim>
                </Area>
                <Image type="TopLeftCorner" imageset="Sunrise" image="SimpleFrameTopLeft" />
                <Image type="TopRightCorner" imageset="Sunrise" image="SimpleFrameTopRight" />
                <Image type="BottomLeftCorner" imageset="Sunrise" image="SimpleFrameBottomLeft" />
                <Image type="BottomRightCorner" imageset="Sunrise" image="SimpleFrameBottomRight" />
                <Image type="LeftEdge" imageset="Sunrise" image="SimpleFrameLeft" />
                <Image type="TopEdge" imageset="Sunrise" image="SimpleFrameTop" />
                <Image type="RightEdge" imageset="Sunrise" image="SimpleFrameRight" />
                <Image type="BottomEdge" imageset="Sunrise" image="SimpleFrameBottom" />
            </FrameComponent>
        </ImagerySection>

        <ImagerySection name="background_frame">
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge">
                        <ImageDim imageset="Sunrise" image="SimpleFrameLeft" dimension="Width" />
                    </Dim>
                    <Dim type="TopEdge">
                        <ImageDim imageset="Sunrise" image="SimpleFrameTop" dimension="Height" />
                    </Dim>
                    <Dim type="RightEdge">
                        <UnifiedDim scale="1" type="RightEdge">
                            <DimOperator op="Subtract">
                                <ImageDim imageset="Sunrise" image="SimpleFrameRight" dimension="Width" />
                            </DimOperator>
                        </UnifiedDim>
                    </Dim>
                    <Dim type="BottomEdge">
                        <UnifiedDim scale="1" type="BottomEdge">
                            <DimOperator op="Subtract">
                                <ImageDim imageset="Sunrise" image="SimpleFrameBottom" dimension="Height" />
                            </DimOperator>
                        </UnifiedDim>
                    </Dim>
                </Area>
                <Image imageset="Sunrise" image="WhiteBrush" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>
        </ImagerySection>

        <ImagerySection name="background_noframe">
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="TopEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="RightEdge"><UnifiedDim scale="1" type="RightEdge" /></Dim>
                    <Dim type="BottomEdge"><UnifiedDim scale="1" type="BottomEdge" /></Dim>
                </Area>
                <Image imageset="Sunrise" image="WhiteBrush" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>
        </ImagerySection>

    </WidgetLook>

	<!--
	********************************************************
	Sunrise/Button
	********************************************************
	-->
    <WidgetLook name="Sunrise/Button">
 
		<PropertyDefinition name="NormalTextColour" initialValue="FFFFFFFF" redrawOnWrite="true" />
        <PropertyDefinition name="HoverTextColour" initialValue="66666666" redrawOnWrite="true" />
        <PropertyDefinition name="PushedTextColour" initialValue="FFFFFFFF" redrawOnWrite="true" />
        <PropertyDefinition name="DisabledTextColour" initialValue="66666600" redrawOnWrite="true" />
        <PropertyDefinition name="HoverColour" initialValue="FFFFFFFF" redrawOnWrite="true" />
        <PropertyDefinition name="PushedColour" initialValue="808080FF" redrawOnWrite="true" />
        <PropertyDefinition name="DisabledColour" initialValue="80808080" redrawOnWrite="true" />
        <PropertyDefinition name="VertLabelFormatting" initialValue="CentreAligned" />
        <PropertyDefinition name="HorzLabelFormatting" initialValue="CentreAligned" />
		<Property name="Font" value="SmallFont" />
		
        <ImagerySection name="label">
            <TextComponent>
                <Area>
                    <Dim type="LeftEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="TopEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="Width"><UnifiedDim scale="1" type="Width" /></Dim>
                    <Dim type="Height"><UnifiedDim scale="1" type="Height" /></Dim>
                </Area>
                <VertFormatProperty name="VertLabelFormatting" />
                <HorzFormatProperty name="HorzLabelFormatting" />
            </TextComponent>
        </ImagerySection>
		
		<ImagerySection name="normal_image">
            <FrameComponent>
                <Area>
                    <Dim type="LeftEdge">
                        <AbsoluteDim value="0" />
                    </Dim>
                    <Dim type="TopEdge">
                        <AbsoluteDim value="0" />
                    </Dim>
                    <Dim type="RightEdge">
                        <UnifiedDim scale="1" type="RightEdge" />
                    </Dim>
                    <Dim type="BottomEdge">
                        <UnifiedDim scale="1" type="BottomEdge" />
                    </Dim>
                </Area>
                <Image type="TopLeftCorner" imageset="Sunrise" image="ButtonNormalTopLeft" />
                <Image type="TopRightCorner" imageset="Sunrise" image="ButtonNormalTopRight" />
                <Image type="BottomLeftCorner" imageset="Sunrise" image="ButtonNormalBottomLeft" />
                <Image type="BottomRightCorner" imageset="Sunrise" image="ButtonNormalBottomRight" />
                <Image type="LeftEdge" imageset="Sunrise" image="ButtonNormalLeft" />
                <Image type="TopEdge" imageset="Sunrise" image="ButtonNormalTop" />
                <Image type="RightEdge" imageset="Sunrise" image="ButtonNormalRight" />
                <Image type="BottomEdge" imageset="Sunrise" image="ButtonNormalBottom" />
            </FrameComponent>
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge">
                        <ImageDim imageset="Sunrise" image="ButtonNormalLeft" dimension="Width" />
                    </Dim>
                    <Dim type="TopEdge">
                        <ImageDim imageset="Sunrise" image="ButtonNormalTop" dimension="Height" />
                    </Dim>
                    <Dim type="RightEdge">
                        <UnifiedDim scale="1" type="RightEdge">
                            <DimOperator op="Subtract">
                                <ImageDim imageset="Sunrise" image="ButtonNormalRight" dimension="Width" />
                            </DimOperator>
                        </UnifiedDim>
                    </Dim>
                    <Dim type="BottomEdge">
                        <UnifiedDim scale="1" type="BottomEdge">
                            <DimOperator op="Subtract">
                                <ImageDim imageset="Sunrise" image="ButtonNormalBottom" dimension="Height" />
                            </DimOperator>
                        </UnifiedDim>
                    </Dim>
                </Area>
                <Image imageset="Sunrise" image="ButtonNormalBrush" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>
		</ImagerySection>
		
		<ImagerySection name="hover_image">
            <FrameComponent>
                <Area>
                    <Dim type="LeftEdge">
                        <AbsoluteDim value="0" />
                    </Dim>
                    <Dim type="TopEdge">
                        <AbsoluteDim value="0" />
                    </Dim>
                    <Dim type="RightEdge">
                        <UnifiedDim scale="1" type="RightEdge" />
                    </Dim>
                    <Dim type="BottomEdge">
                        <UnifiedDim scale="1" type="BottomEdge" />
                    </Dim>
                </Area>
                <Image type="TopLeftCorner" imageset="Sunrise" image="ButtonHoverTopLeft" />
                <Image type="TopRightCorner" imageset="Sunrise" image="ButtonHoverTopRight" />
                <Image type="BottomLeftCorner" imageset="Sunrise" image="ButtonHoverBottomLeft" />
                <Image type="BottomRightCorner" imageset="Sunrise" image="ButtonHoverBottomRight" />
                <Image type="LeftEdge" imageset="Sunrise" image="ButtonHoverLeft" />
                <Image type="TopEdge" imageset="Sunrise" image="ButtonHoverTop" />
                <Image type="RightEdge" imageset="Sunrise" image="ButtonHoverRight" />
                <Image type="BottomEdge" imageset="Sunrise" image="ButtonHoverBottom" />
            </FrameComponent>
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge">
                        <ImageDim imageset="Sunrise" image="ButtonHoverLeft" dimension="Width" />
                    </Dim>
                    <Dim type="TopEdge">
                        <ImageDim imageset="Sunrise" image="ButtonHoverTop" dimension="Height" />
                    </Dim>
                    <Dim type="RightEdge">
                        <UnifiedDim scale="1" type="RightEdge">
                            <DimOperator op="Subtract">
                                <ImageDim imageset="Sunrise" image="ButtonHoverRight" dimension="Width" />
                            </DimOperator>
                        </UnifiedDim>
                    </Dim>
                    <Dim type="BottomEdge">
                        <UnifiedDim scale="1" type="BottomEdge">
                            <DimOperator op="Subtract">
                                <ImageDim imageset="Sunrise" image="ButtonHoverBottom" dimension="Height" />
                            </DimOperator>
                        </UnifiedDim>
                    </Dim>
                </Area>
                <Image imageset="Sunrise" image="ButtonHoverBrush" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>
		</ImagerySection>
		
		<StateImagery name="Normal">
		<Layer>
			<Section section="normal_image" />
			<Section section="label">
                    <ColourProperty name="NormalTextColour" />
            </Section>		
		</Layer>
		</StateImagery>
		
		<StateImagery name="Hover">
		<Layer>
			<Section section="hover_image">
				<ColourProperty name="HoverColour" />
			</Section>
			<Section section="label">
                <ColourProperty name="HoverTextColour" />
            </Section>		
		</Layer>
		</StateImagery>
		
		<StateImagery name="Pushed">
		<Layer>
			<Section section="hover_image">
				<ColourProperty name="PushedColour" />
			</Section>
			<Section section="label">
                <ColourProperty name="PushedTextColour" />
            </Section>		
		</Layer>
		</StateImagery>
		
		<StateImagery name="Disabled">
		<Layer>
			<Section section="hover_image">
				<ColourProperty name="DisabledColour" />
			</Section>
			<Section section="label">
                <ColourProperty name="DisabledTextColour" />
            </Section>		
		</Layer>
		</StateImagery>

    </WidgetLook>
	
	<!--
    ***************************************************
    Sunrise/IconButton
    ***************************************************
    -->
    <WidgetLook name="Sunrise/IconButton">
	
        <PropertyDefinition name="IconImage" redrawOnWrite="true" />
        <PropertyDefinition name="IconColours" initialValue="tl:FFFFFFFF tr:FFFFFFFF bl:FFFFFFFF br:FFFFFFFF" redrawOnWrite="true" />
		<PropertyDefinition name="PushedOverlay" initialValue="tl:888888FF tr:888888FF bl:888888FF br:888888FF" redrawOnWrite="true" />
        <Property name="WantsMultiClickEvents" value="False" />
		
		<ImagerySection name="background_active">
			<ImageryComponent>
				<Area>
                    <Dim type="TopEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="LeftEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="RightEdge"><UnifiedDim scale="1" type="RightEdge" /></Dim>
                    <Dim type="BottomEdge"><UnifiedDim scale="1" type="BottomEdge" /></Dim>
                </Area>
                <Image imageset="Sunrise" image="IconbuttonNormal" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>
        </ImagerySection>

		<ImagerySection name="background_hover">
			<ImageryComponent>
				<Area>
                    <Dim type="TopEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="LeftEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="RightEdge"><UnifiedDim scale="1" type="RightEdge" /></Dim>
                    <Dim type="BottomEdge"><UnifiedDim scale="1" type="BottomEdge" /></Dim>
                </Area>
                <Image imageset="Sunrise" image="IconbuttonHover" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>
        </ImagerySection>

		<ImagerySection name="background_disabled">
			<ImageryComponent>
				<Area>
                    <Dim type="TopEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="LeftEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="RightEdge"><UnifiedDim scale="1" type="RightEdge" /></Dim>
                    <Dim type="BottomEdge"><UnifiedDim scale="1" type="BottomEdge" /></Dim>
                </Area>
                <Image imageset="Sunrise" image="IconbuttonDisabled" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>
        </ImagerySection>

        <ImagerySection name="icon">
            <ImageryComponent>
                <Area>
                    <Dim type="TopEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="LeftEdge"><AbsoluteDim value="0" /></Dim>
                    <Dim type="RightEdge"><UnifiedDim scale="1" type="RightEdge" /></Dim>
                    <Dim type="BottomEdge"><UnifiedDim scale="1" type="BottomEdge" /></Dim>
                </Area>
                <ImageProperty name="IconImage" />
                <ColourRectProperty name="IconColours" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>
        </ImagerySection>
		
        <StateImagery name="Normal">
            <Layer>
                <Section section="background_active" />
                <Section section="icon" />
            </Layer>
        </StateImagery>
		
        <StateImagery name="Hover">
            <Layer>
                <Section section="background_hover" />
                <Section section="icon" />
            </Layer>
        </StateImagery>
		
        <StateImagery name="Pushed">
            <Layer>
                <Section section="background_active">
					<ColourRectProperty name="PushedOverlay" />
				</Section>
                <Section section="icon" />
            </Layer>
        </StateImagery>
		
        <StateImagery name="Disabled">
            <Layer>
                <Section section="background_disabled" />
                <Section section="icon" />
            </Layer>
        </StateImagery>
		
    </WidgetLook>

